  "types": "dist/index.d.ts",
  "peerDependencies": {
    "rxjs": "^7.8.1"
  },
  "keywords": [
    "Rx",
    "RxJS",
    "ReactiveExtensions",
    "Observable",
    "Observables",
    "distinctUntilChanged",
    "distinctUntilChangedDeep"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonukapoor/distinctUntilChangedDeep.git"
  },
  "author": "Sonu Kapoor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonukapoor/distinctUntilChangedDeep/issues"
  },
  "homepage": "https://github.com/sonukapoor/distinctUntilChangedDeep#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
